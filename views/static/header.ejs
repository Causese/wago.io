<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="<%= site.description %>">
    <meta property="og:site_name" content="WAGO" >
    <meta property="og:title" content="<%= site.title %>">
    <%
    if (aura && aura.screens && aura.screens.length>0) {
        %><meta property="og:image" content="<%= aura.screens[0].url.original  %>"><%
    }
    %>
    <link rel="shortcut icon" href="/assets/img/favicon.ico" />

    <title><%= site.title || 'WAGO.IO '%></title>

    <%- CDN([ '/css/bootstrap.css', 
	'/css/font-awesome.min.css', 
	'/libraries/lightbox2/dist/css/lightbox.min.css',
	'/libraries/selectize/css/selectize.bootstrap3.css',
	'/css/bootstrap-slider.css',
	'/css/wago.css', 
	'/css/wago-ic.css']).replace(/\+/g, '%2B') %>
    <link href='https://fonts.googleapis.com/css?family=Roboto:400,700' rel='stylesheet' type='text/css'>
</head>